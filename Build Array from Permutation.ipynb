    "class Solution:\n",
    "    def arraybuild(Self,a):\n",
    "        temp=list()\n",
    "        print(\"Before--\",a)\n",
    "        for i in range(0,len(a)):\n",
    "            id=a[i]\n",
    "            temp.append(a[id])\n",
    "        print(\"After\",temp)    \n",
    "        \n",
    "        \n",
    "s=Solution()\n",
    "a=[0,2,1,4,3,6,5]\n",
    "s.arraybuild(a)\n",
    "        "
