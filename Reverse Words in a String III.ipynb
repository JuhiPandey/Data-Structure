    "class Solution:\n",
    "\n",
    "    def reverseWords(self, s: str) -> str:\n",
    "        s=s.split()\n",
    "        for i in range(len(s)):\n",
    "            s[i]=s[i][::-1]\n",
    "        return \" \".join(s)\n",
    "        \n",
    "o=Solution()\n",
    "s = \"Let's take LeetCode contest\"\n",
    "o.reverseWords(s)"
   ]
